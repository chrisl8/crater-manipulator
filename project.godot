config_version=5

[application]

config/name="Crater Manipulator"
run/main_scene="res://startup.tscn"
config/features=PackedStringArray("4.2")
boot_splash/image="res://splash-screen.png"

[autoload]

Globals="*res://global_variables.gd"
Helpers="*res://helper_functions.gd"
Network="*res://network_websocket.gd"
Spawner="*res://spawner.gd"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1
gdscript/warnings/unsafe_cast=1

[display]

window/size/viewport_width=576
window/size/always_on_top=true
mouse_cursor/custom_image="res://items/mouse_cursor.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/jwt/plugin.cfg")

